    <TextView
        android:id="@+id/tv_id_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#212121" />

    <TextView
        android:id="@+id/tv_nama_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#212121" />
</LinearLayout>